{
  "commandRules": {
    "allow": [
      "yarn workspace",
      "yarn workspace list",
      "yarn workspace run",
      "yarn workspace build",
      "yarn workspace test",
      "yarn workspace type-check",
      "yarn workspace lint",
      "yarn workspace expo-app",
      "yarn workspace next-app",
      "yarn workspace @my/ui",
      "yarn workspace @my/app",
      "yarn workspace @my/api",
      "yarn workspace @my/config"
    ],
    "block": [
      "yarn workspace add",
      "yarn workspace remove",
      "yarn workspace expo-app add",
      "yarn workspace expo-app remove",
      "yarn workspace next-app add",
      "yarn workspace next-app remove",
      "yarn workspace @my/ui add",
      "yarn workspace @my/ui remove",
      "yarn workspace @my/app add",
      "yarn workspace @my/app remove",
      "yarn workspace @my/api add",
      "yarn workspace @my/api remove",
      "yarn workspace @my/config add",
      "yarn workspace @my/config remove",
      "yarn supabase db reset",
      "yarn supabase db reset --linked",
      "yarn supabase db reset --debug",
      "yarn supabase db reset --no-seed",
      "yarn supabase db reset --local",
      "yarn dlx supabase db reset",
      "yarn dlx supabase db reset --linked",
      "yarn dlx supabase db reset --debug",
      "yarn dlx supabase db reset --no-seed",
      "yarn dlx supabase db reset --local",
      "echo * > .env",
      "echo * >> .env",
      "echo * > .env.local",
      "echo * >> .env.local",
      "echo * > .env.development",
      "echo * >> .env.development",
      "echo * > .env.production",
      "echo * >> .env.production",
      "perl -i *",
      "perl -i.bak *",
      "perl -pi *",
      "perl -pi.bak *",
      "cat * > *",
      "cat * >> *",
      "python * > *",
      "python * >> *"
    ]
  },
  "validation": {
    "enabled": true,
    "behavior": "confirmation-required"
  }
}