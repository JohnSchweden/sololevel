{
  "commandRules": {
    "allow": [
      "yarn workspace",
      "yarn workspace list",
      "yarn workspace run",
      "yarn workspace build",
      "yarn workspace test",
      "yarn workspace type-check",
      "yarn workspace lint",
      "yarn workspace expo-app",
      "yarn workspace next-app",
      "yarn workspace @my/ui",
      "yarn workspace @my/app",
      "yarn workspace @my/api",
      "yarn workspace @my/config"
    ],
    "block": [
      "yarn workspace * add *",
      "yarn workspace * remove *",
      "yarn supabase db reset*",
      "yarn dlx supabase db reset*",
      "echo * > .env",
      "echo * >> .env",
      "echo * > .env.local",
      "echo * >> .env.local",
      "echo * > .env.development",
      "echo * >> .env.development",
      "echo * > .env.production",
      "echo * >> .env.production",
      "perl -i *",
      "perl -i.bak *",
      "perl -pi *",
      "perl -pi.bak *",
      "cat * > *",
      "cat * >> *",
      "python * > *",
      "python * >> *",
      "npx tsc*",
      "tsc *"
    ]
  },
  "validation": {
    "enabled": true,
    "behavior": "confirmation-required"
  }
}