{
  "name": "@my/ui",
  "version": "0.0.1",
  "sideEffects": [
    "*.css"
  ],
  "private": true,
  "types": "./src",
  "main": "src/index.tsx",
  "module:jsx": "src",
  "files": [
    "types",
    "dist"
  ],
  "scripts": {
    "build": "tamagui-build --skip-types",
    "watch": "tamagui-build --skip-types --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:coverage:report": "vitest run --coverage --reporter=html"
  },
  "dependencies": {
    "@my/config": "0.0.1",
    "@tamagui/toast": "1.132.20"
  },
  "devDependencies": {
    "@tamagui/build": "1.132.20",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.6",
    "tamagui": "1.132.20"
  },
  "peerDependencies": {
    "react": "^18 || ^19",
    "react-dom": "^18 || ^19",
    "react-native": "0.79.6",
    "tamagui": "1.132.20"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    },
    "react-native": {
      "optional": true
    }
  }
}
