{
  "name": "@my/ui",
  "version": "0.0.1",
  "sideEffects": [
    "*.css"
  ],
  "private": true,
  "types": "./src",
  "main": "src/index.ts",
  "module:jsx": "src",
  "files": [
    "types",
    "dist"
  ],
  "scripts": {
    "test:fast": "jest --passWithNoTests --no-coverage",
    "build": "echo 'ui package - built by tsc in root'",
    "lint": "biome check src",
    "lint:fix": "biome check --write src",
    "watch": "tamagui-build --skip-types --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:coverage:report": "jest --coverage --coverageReporters=html",
    "test:native": "jest --config jest.native.config.js",
    "test:native:watch": "jest --config jest.native.config.js --watch",
    "test:native:coverage": "jest --config jest.native.config.js --coverage",
    "test:all": "jest && jest --config jest.native.config.js"
  },
  "dependencies": {
    "@expo/react-native-action-sheet": "^4.1.1",
    "@my/config": "0.0.1",
    "@shopify/react-native-skia": "v2.0.0-next.4",
    "@tamagui/core": "1.132.20",
    "@tamagui/lucide-icons": "1.132.20",
    "@tamagui/toast": "1.132.20",
    "@types/react": "^19.1.12",
    "expo": "53.0.22",
    "expo-camera": "^16.1.11",
    "expo-document-picker": "^13.1.6",
    "expo-file-system": "^18.0.4",
    "expo-image-picker": "^16.1.4",
    "expo-linking": "^7.1.7",
    "react-native-logs": "^5.3.0",
    "react-native-svg": "^15.12.1",
    "react-native-vision-camera": "^4.7.1",
    "react-native-worklets-core": "^1.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/plugin-transform-optional-chaining": "^7.27.1",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@tamagui/build": "1.132.20",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/react-native": "^13.3.3",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.6",
    "react-native-web": "^0.21.0",
    "react-test-renderer": "19.0.0",
    "tamagui": "1.132.20"
  },
  "peerDependencies": {
    "react": "^18 || ^19",
    "react-dom": "^18 || ^19",
    "react-native": "0.79.6",
    "react-native-video": ">=6.16.1",
    "tamagui": "1.132.20"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    },
    "react-native": {
      "optional": true
    }
  }
}
