# AI API
GEMINI_API_KEY=your_api_key
GEMINI_MMM_MODEL=gemini-1.5-flash
GEMINI_LLM_MODEL=gemini-2.5-flash
GEMINI_TTS_MODEL=gemini-2.5-flash-preview-tts
GEMINI_FILES_MAX_MB=20
DEFAULT_VOICE_NAME=Sadachbia

# AI Analysis Mode: 'real' for actual API calls, 'mock' for prepared responses (MVP testing)
AI_ANALYSIS_MODE=real

# Pipeline Stages Configuration
# JSON configuration for which pipeline stages to run (applies to both real and mock modes)
# If not set, all stages will run by default
# Example: {"runVideoAnalysis": true, "runLLMFeedback": false, "runSSML": false, "runTTS": false}
PIPELINE_STAGES={"runVideoAnalysis": true, "runLLMFeedback": true, "runSSML": true, "runTTS": true}

# TTS Audio Format Configuration
# Default audio format for TTS generation (mp3, aac)
TTS_DEFAULT_FORMAT=wav
# Comma-separated list of allowed audio formats for TTS
TTS_ALLOWED_FORMATS=wav
# Signed URL TTL in seconds for private audio bucket (default: 900 = 15 minutes)
SUPABASE_SIGNED_URL_TTL_SECONDS=900

GROQ_API_KEY=your_api_key

QWEN_2_5_VL=qwen-2.5-vl

# Supabase Configuration
# Copy this file to .env and fill in your actual values

# Production/Remote Supabase (for deployment)
# Get these from https://supabase.com/dashboard/project/qbkvqhoijishdkqlwhqp/settings/api

# For Expo/React Native (mobile app)
EXPO_PUBLIC_SUPABASE_URL=https://qbkvqhoijishdkqlwhqp.supabase.co
EXPO_PUBLIC_SUPABASE_KEY=your_production_anon_key

# For Next.js (web app)
NEXT_PUBLIC_SUPABASE_URL=https://qbkvqhoijishdkqlwhqp.supabase.co
NEXT_PUBLIC_SUPABASE_PUBLISHABLE_DEFAULT_KEY=your_production_anon_key

# For server/backend operations
SUPABASE_URL=https://qbkvqhoijishdkqlwhqp.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your_production_service_role_key

