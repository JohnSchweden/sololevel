{
  "expo": {
    "name": "Solo:Level",
    "slug": "sololevel",
    "scheme": "sololevel",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1a1a1a"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "ai.sololevel.app",
      "buildNumber": "10",
      "icon": "./assets/icon.png",
      "darkIcon": "./assets/icon-dark.png",
      "tintedIcon": "./assets/icon-tinted.png",
      "infoPlist": {
        "CFBundleIconName": "AppIcon",
        "UIViewControllerBasedStatusBarAppearance": false,
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library to select workout videos for AI analysis.",
        "NSPhotoLibraryAddUsageDescription": "This app needs access to save videos to your photo library, just like the native camera app.",
        "NSLocationWhenInUseUsageDescription": "This app does not use your location. This permission may be referenced by system libraries for photo metadata access.",
        "ITSAppUsesNonExemptEncryption": false,
        "RCTHermesGCConfig": {
          "InitHeapSize": 67108864,
          "MaxHeapSize": 536870912,
          "SanitizeConfig": {
            "RandomSeed": 0
          }
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#f3f3f3"
      },
      "package": "ai.sololevel.app",
      "versionCode": 12,
      "splash": {
        "image": "./assets/splashscreen_logo.png",
        "resizeMode": "contain",
        "backgroundColor": "#1a1a1a"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.READ_MEDIA_VIDEO",
        "android.permission.READ_MEDIA_IMAGES"
      ],
      "statusBar": {
        "translucent": true,
        "backgroundColor": "transparent"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static",
            "deploymentTarget": "18.4"
          }
        }
      ],
      [
        "react-native-vision-camera",
        {
          "cameraPermissionText": "This app needs access to camera to record workout videos and analyze your form with AI pose detection.",
          "enableMicrophonePermission": true,
          "microphonePermissionText": "This app needs access to microphone to record audio with your workout videos."
        }
      ],
      "./plugins/withDisableCodeSigning",
      "./plugins/withDarkIcon",
      "./plugins/withJavaVersion",
      [
        "@sentry/react-native/expo",
        {
          "organization": "sololevel",
          "project": "sololevel-app",
          "uploadNativeSymbols": true,
          "uploadSourceMaps": true,
          "setCommits": false,
          "dsn": "https://6718ebeaf9ca5a552b765cb1d32857cb@o4510125964787712.ingest.de.sentry.io/4510126031503440"
        }
      ],
      "./plugins/withDisableSentryUpload",
      "./plugins/withDisableSentryUploadGradle"
    ],
    "experiments": {
      "typedRoutes": true,
      "newArchEnabled": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "cf2a6fd7-0d34-495d-9ffe-a822d0ebcbae"
      }
    }
  }
}
