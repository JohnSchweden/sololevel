{
  "name": "expo-app",
  "version": "1.0.0",
  "main": "index.js",
  "private": true,
  "installConfig": {
    "hoistingLimits": "workspaces"
  },
  "scripts": {
    "build": "echo 'Skipping native build - TypeScript compilation only'",
    "start": "EXPO_NO_TELEMETRY=1 expo start -c",
    "android": "EXPO_NO_TELEMETRY=1 expo run:android",
    "android:release": "cd android && SENTRY_AUTH_TOKEN=${SENTRY_AUTH_TOKEN:-} ./gradlew installRelease",
    "ios": "yarn fix-xcode-env && EXPO_NO_TELEMETRY=1 expo run:ios",
    "eject": "expo eject",
    "fix-xcode-env": "node scripts/fix-xcode-env.mjs",
    "prebuild": "expo prebuild",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "type-check": "tsc --noEmit",
    "increment-build": "node scripts/increment-build-number.mjs",
    "eas:build:ios": "node scripts/build-with-increment.mjs ios production",
    "eas:submit:ios": "yarn dlx eas-cli submit -p ios",
    "eas:build:android": "node scripts/build-with-increment.mjs android production",
    "eas:submit:android": "yarn dlx eas-cli submit -p android"
  },
  "dependencies": {
    "@expo-google-fonts/josefin-sans": "^0.4.2",
    "@expo/config-plugins": "~10.1.1",
    "@expo/react-native-action-sheet": "^4.1.1",
    "@my/api": "workspace:*",
    "@my/app": "workspace:*",
    "@my/config": "0.0.1",
    "@my/logging": "workspace:*",
    "@my/ui": "0.0.1",
    "@react-native-masked-view/masked-view": "^0.3.2",
    "@react-navigation/native": "^7.1.7",
    "@sentry/react-native": "^7.8.0",
    "@tamagui/font-inter": "1.135.2",
    "@tanstack/react-query": "^5.85.5",
    "babel-plugin-module-resolver": "^5.0.2",
    "burnt": "^0.13.0",
    "expo": "~53.0.23",
    "expo-application": "^7.0.8",
    "expo-build-properties": "^0.14.8",
    "expo-constants": "~17.1.7",
    "expo-dev-client": "^5.2.4",
    "expo-device": "^8.0.10",
    "expo-document-picker": "^13.1.6",
    "expo-file-system": "^18.0.4",
    "expo-font": "~13.3.2",
    "expo-image-picker": "^17.0.7",
    "expo-linear-gradient": "^15.0.7",
    "expo-linking": "^7.1.7",
    "expo-localization": "^16.1.6",
    "expo-router": "~5.1.7",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-updates": "~0.28.17",
    "expo-video-thumbnails": "^10.0.7",
    "i18next": "^25.4.2",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-i18next": "^15.7.2",
    "react-native": "0.79.6",
    "react-native-compressor": "1.13.0",
    "react-native-gesture-handler": "^2.28.0",
    "react-native-logs": "^5.3.0",
    "react-native-mmkv": "^4.1.0",
    "react-native-nitro-modules": "^0.32.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "^5.6.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "^15.12.1",
    "react-native-video": "^6.16.1",
    "react-native-vision-camera": "^4.7.1",
    "sentry-expo": "^7.2.0",
    "tamagui": "1.135.2"
  },
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@expo/metro-config": "~0.20.0",
    "@tamagui/babel-plugin": "1.135.2",
    "@types/jest": "^30.0.0",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19",
    "@types/react-test-renderer": "^19",
    "console-browserify": "^1.2.0",
    "expo-doctor": "1.13.5",
    "jest": "^29.7.0",
    "jest-expo": "^53.0.9",
    "jsdom": "^26.1.0",
    "metro-minify-terser": "^0.81.0",
    "process": "^0.11.10",
    "react-test-renderer": "19.0.0",
    "ts-jest": "^29.4.1",
    "typescript": "^5.9.2",
    "util": "^0.12.5",
    "webpack": "^5.79.0"
  }
}
