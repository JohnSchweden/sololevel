{
  "$schema": "https://turborepo.org/schema.json",
  "globalEnv": [
    "DISABLE_EXTRACTION",
    "NODE_ENV",
    "EAS_BUILD_PLATFORM",
    "NEXT_PUBLIC_SUPABASE_URL",
    "NEXT_PUBLIC_SUPABASE_PUBLISHABLE_DEFAULT_KEY"
  ],
  "pipeline": {
    "build": {
      "env": [
        "DISABLE_EXTRACTION",
        "NODE_ENV",
        "EAS_BUILD_PLATFORM",
        "NEXT_PUBLIC_SUPABASE_URL",
        "NEXT_PUBLIC_SUPABASE_PUBLISHABLE_DEFAULT_KEY"
      ],
      "dependsOn": ["^build"],
      "outputs": [
        ".next/**",
        "!.next/cache/**",
        "build/**",
        "node_modules/.cache/metro/**",
        "ios/**",
        "android/**"
      ]
    },
    "type-check": {
      "dependsOn": ["^build"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "native": {
      "cache": false,
      "persistent": true
    }
  }
}
